{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "Decision Tree Predictor Properties",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "FileSet Name",
          "name": "fileSetName"
        },
        {
          "widget-type": "textbox",
          "label": "Path",
          "name": "path"
        },
        {
          "widget-type": "csv",
          "label": "Features to Include for Prediction",
          "name": "featureFieldsToInclude",
          "widget-attributes": {
            "delimiter": ",",
            "value-placeholder": "Field Name"
          }
        },
        {
          "widget-type": "csv",
          "label": "Features to Exclude while Prediction",
          "name": "featureFieldsToExclude",
          "widget-attributes": {
            "delimiter": ",",
            "value-placeholder": "Field Name"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Prediction Field",
          "name": "predictionField"
        }
      ]
    }
  ],
  "outputs": []
}
